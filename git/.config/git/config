[alias]
	st = status
	ci = commit
	co = checkout
	pl = pull --rebase
	lg = log --graph --full-history --all --color --pretty=format:\"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s\"
	rc = rebase --continue
	s = status
[core]
	editor = vim
[push]
	default = simple
[merge]
	tool = vimdiff
[rebase]
	autosquash = true
[commit]
	template = /home/.config/git/commit-message-template.txt
[advice]
	deatchedHead = false
